##################################

record (mbbiDirect, "PANDA:$(subsys):VME$(ID):Statusbyte0") {
  field (DTYP, "asynUInt32Digital")
  field (INP,  "@asynMask($(BUS),0,0xff,1)WIENER_VME_STATUS0")
  field (FLNK, "PANDA:$(subsys):VME$(ID):LV:C0:Vmom")
}
