### Read measured Voltage and Current of channel 0 (channel 4 not connected)

record (ai, "PANDA:$(subsys):VME$(ID):LV:C0:Vmom") {
  field (DTYP, "asynInt32")
  field (INP,  "@asyn($(BUS),0,1)WIENER_VME_VMOM0")
  field (FLNK, "PANDA:$(subsys):VME$(ID):LV:C1:Vmom")
  # Conversion
  field (ASLO, "0.01")
  field (AOFF, "0")
  # display parameters
  field (EGU,  "V")
  field (PREC, "2")
  # alarm/warning parameters
  field (HIHI, "5.4")
  field (HIGH, "5.2")
  field (LOW,  "4.8")
  field (LOLO, "4.6")
  field (HHSV, "MAJOR")
  field (HSV,  "MINOR")
  field (LSV,  "MINOR")
  field (LLSV, "MAJOR")
}

record (ai, "PANDA:$(subsys):VME$(ID):LV:C0:Imom") {
  field (DTYP, "asynInt32")
  field (INP,  "@asyn($(BUS),0,1)WIENER_VME_IMOM0")
  field (SCAN, "I/O Intr")
  # Conversion
  field (ASLO, "0.01")
  field (AOFF, "0")
  # display parameters
  field (EGU,  "A")
  field (PREC, "2")
  # alarm/warning parameters
  field (HIHI, "14")
  field (HIGH, "12")
  field (LOW,  "0")
  field (LOLO, "0")
  field (HHSV, "MAJOR")
  field (HSV,  "MINOR")
  field (LSV,  "NO_ALARM")
  field (LLSV, "NO_ALARM")
}

